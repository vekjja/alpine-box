{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "type": "python",
                    "style": "diamond",
                    "foreground": "#100e23",
                    "background": "#906cff",
                    "leading_diamond": "  \ue0b2",
                    "trailing_diamond": "\ue0b0",
                    "template": " {{ .Venv }} "
                },
                {
                    "type": "python",
                    "style": "diamond",
                    "foreground": "#000000",
                    "background": "#ffea4d",
                    "leading_diamond": "<transparent,background>\ue0b0</>",
                    "trailing_diamond": "\ue0b0",
                    "newline": true,
                    "template": " {{ .Full }} "
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#a85cff",
                    "foreground": "#000000",
                    "style": "diamond",
                    "leading_diamond": "  \ue0b2",
                    "trailing_diamond": "\ue0b0",
                    "template": "󰠳 {{ .Context }} ",
                    "type": "kubectl"
                },
                {
                    "background": "#dcff30",
                    "foreground": "#000000",
                    "properties": {
                        "min_width": 10,
                        "max_width": 3,
                        "context_aliases": {
                            "CLUSTER NAME": "\ued97"
                        }
                    },
                    "style": "diamond",
                    "leading_diamond": "<transparent,background>\ue0b0</>",
                    "trailing_diamond": "\ue0b0",
                    "template": "{{ if .Namespace }} {{ .Namespace }}{{ end }} ",
                    "type": "kubectl"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "background": "#16b9ff",
                    "foreground": "#000000",
                    "properties": {
                        "style": "agnoster_full",
                        "folder_icon": "\u2026"
                    },
                    "style": "diamond",
                    "leading_diamond": "\u256d\u2500\ue0b2",
                    "trailing_diamond": "\ue0b0",
                    "powerline_symbol": "\ue0b0",
                    "template": "󰝰 {{ .Path }} ",
                    "type": "path"
                },
                {
                    "background": "#35e7b7",
                    "foreground": "#000000",
                    "background_templates": [
                        "{{ if or (.Working.Changed) (.Staging.Changed) }}#e7357c{{ end }}",
                        "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#54433a{{ end }}",
                        "{{ if gt .Ahead 0 }}#ffaf00{{ end }}",
                        "{{ if gt .Behind 0 }}#424242{{ end }}"
                    ],
                    "properties": {
                        "branch_max_length": 25,
                        "fetch_stash_count": true,
                        "fetch_status": true,
                        "branch_icon": "\ue725 ",
                        "branch_identical_icon": "\uf444",
                        "branch_gone_icon": "\ueab8"
                    },
                    "style": "diamond",
                    "leading_diamond": "<transparent,background>\ue0b0</>",
                    "template": "{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} <transparent>\ue0b1</> <#121318>\uf044 {{ .Working.String }}</>{{ end }}{{ if .Staging.Changed }} <transparent>\ue0b1</> <#121318>\uf046 {{ .Staging.String }}</>{{ end }}{{ if gt .StashCount 0 }} <transparent>\ue0b1</> <#121318>\ueb4b {{ .StashCount }}</>{{ end }} ",
                    "trailing_diamond": "\ue0b0",
                    "type": "git"
                },
                {
                    "background": "#424242",
                    "foreground": "#f2f3f8",
                    "properties": {
                        "style": "austin",
                        "max_depth": 3,
                        "folder_icon": "\u2026",
                        "folder_separator_icon": " <transparent>\ue0b1</> ",
                        "always_enabled": true
                    },
                    "style": "powerline",
                    "powerline_symbol": "\ue0b0",
                    "template": " {{ .FormattedMs }} ",
                    "type": "executiontime"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#00b3ff",
                    "template": "╰ ",
                    "style": "plain",
                    "type": "text"
                },
                {
                    "foreground": "#00b3ff",
                    "foreground_templates": [
                        "{{ if .SSHSession }}#ff6ad5{{ end }}"
                    ],
                    "template": "{{ if .SSHSession }}󰅟{{ else }}{{ end }}  ",
                    "style": "plain",
                    "type": "session"
                },
                {
                    "foreground": "#00b3ff",
                    "template": "{{.UserName}}",
                    "style": "plain",
                    "type": "session"
                },
                {
                    "foreground": "#888888",
                    "style": "plain",
                    "properties": {
                        "kali": ""
                    },
                    "template": " {{ .Icon }} ",
                    "type": "os"
                },
                {
                    "foreground": "#00b3ff",
                    "template": "{{ .HostName }}:",
                    "style": "plain",
                    "type": "session"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 3
}